{
    "name": "@ns-world-lab-kxn/source",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "serve:web-app": "npm run clean && nx serve web-app --host=lab36 --port=8090 --watch",
        "build:web-app": "cls && nx build web-app --watch",
        "build:all": "cls && nx run-many -t build --watch",
        "__serve:admin": "npm run clean && nx serve admin --host=localhost --watch",
        "__build:admin": "cls && nx build admin --watch",
        "---------- tsc": "----------------------------------------",
        "clean": "tsc -b tsconfig.build.json --clean",
        "tsc:types:w": "tsc -b packages/types -w",
        "tsc:logic:w": "tsc -b packages/logic -w",
        "tsc:web:w": "tsc -b packages/web -w",
        "tsc:web-app:w": "tsc -b apps/web-app -w",
        "__b:admin:w": "tsc -b apps/admin -w",
        "---------- nx": "----------------------------------------",
        "nx:types:w": "cls && nx run-many -t build --projects=types --verbose",
        "nx:logic:w": "cls && nx run-many -t build --projects=logic --verbose",
        "nx:web:w": "cls && nx run-many -t build --projects=web --verbose",
        "nx:web-app:w": "cls && nx run-many -t build --projects=web-app --verbose",
        "nx:graph": "cls && nx graph --watch --host=localhost --port=4300",
        "reset": "cls && nx reset",
        "__nx:admin:w": "cls && nx run-many -t build --projects=admin --verbose"
    },
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@faker-js/faker": "^10.0.0",
        "mobx": "^6.13.7",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-rnd": "^10.5.2",
        "rsuite": "^6.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@eslint/js": "^9.8.0",
        "@nx/eslint": "21.5.2",
        "@nx/eslint-plugin": "21.5.2",
        "@nx/jest": "21.5.2",
        "@nx/js": "22.4.1",
        "@nx/react": "^21.5.2",
        "@nx/rspack": "21.5.2",
        "@nx/vite": "^22.4.1",
        "@nx/web": "22.4.1",
        "@nx/workspace": "21.5.2",
        "@rspack/cli": "^1.5.0",
        "@rspack/core": "^1.5.0",
        "@rspack/dev-server": "^1.1.4",
        "@rspack/plugin-react-refresh": "^1.0.0",
        "@swc-node/register": "~1.9.1",
        "@swc/cli": "~0.6.0",
        "@swc/core": "~1.5.7",
        "@swc/helpers": "~0.5.11",
        "@tailwindcss/postcss": "^4.1.18",
        "@types/node": "^20.0.0",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^4.2.0",
        "@vitest/ui": "^3.0.0",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jsx-a11y": "6.10.1",
        "eslint-plugin-react": "7.35.0",
        "eslint-plugin-react-hooks": "5.0.0",
        "jiti": "2.4.2",
        "jsdom": "~22.1.0",
        "nx": "21.5.2",
        "prettier": "^2.6.2",
        "react-refresh": "~0.14.0",
        "tailwindcss": "^4.1.18",
        "tslib": "^2.3.0",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.40.0",
        "vite": "^7.3.1",
        "vitest": "^3.0.0"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}